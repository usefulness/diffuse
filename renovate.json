{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "forkProcessing": "enabled",
  "assignees": [
    "mateuszkwiecinski"
  ],
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "ignoreDeps": [
    "com.google.protobuf:protobuf-java",
    "com.android.tools.build:aapt2-proto"
  ]
}
